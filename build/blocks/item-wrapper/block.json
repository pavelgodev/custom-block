{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cb/item-wrapper",
  "version": "1.0.0",
  "title": "Item Wrapper",
  "category": "widgets",
  "description": "Item wrapper consists only of item blocks. This block is similar to FAQ section, content can be edited by content writers",
  "supports": {
    "html": false
  },
  "icon": "editor-table",
  "textdomain": "cb-item-wrapper",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "heading": {
      "type": "string",
      "source": "html",
      "selector": "h3",
      "default": ""
    },
    "isCollapsed": {
      "type": "boolean",
      "default": false
    }
  }
}